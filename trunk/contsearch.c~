#include "contsearch.h"
#include <stdio.h>
#include <stdlib.h>

int numbits(char a){

  int n = 0;
  char comp = 1;

  if ((n & comp) == comp){
    n++;
  }

  comp = comp << 1;
  while(comp > 1) {
    comp = comp << 1;
    if ((n & comp) == comp){
      n++;
    }
  }

  return n;

}

int main(){

  char a;


  a = 1;
  printf("%d\n", numbits(a));
  a = 0;
  printf("%d\n", numbits(a));
  a = 9;
  printf("%d\n", numbits(a));
  a = 511;
  printf("%d\n", numbits(a));
  

  while (1){
    scanf(¨%c¨, &a);
    printf("%d\n", numbits(a));
  }

  

  return 0;

}
